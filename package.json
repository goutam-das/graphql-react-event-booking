{
  "name": "graphql-react-event-booking",
  "version": "1.0.0",
  "description": "MERNG Event Booking App",
  "main": "index.js",
  "author": "Goutam Das <goutamdas676@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node build/index.js",
    "start:client": "parcel frontend/index.html --open",
    "start:server": "nodemon --watch backend --exec babel-node backend/index.js",
    "prebuild": "rimraf ./build",
    "build:server": "babel -d ./build ./backend -s",
    "build:client": "parcel build frontend/index.html --out-dir ./build/client",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^3.0.2",
    "sass": "^1.26.9"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@types/react": "^16.8.0",
    "antd": "^4.3.5",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.15.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "^2.10.3",
    "helmet": "^3.23.3",
    "lodash": "^4.17.15",
    "react": ">=15",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.2.0"
  }
}